{
  "name": "@lentra/common",
  "version": "0.1.0",
  "description": "Shared types and utilities for Lentra",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./src/types/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
